{
  "name": "crm-backend",
  "version": "1.0.0",
  "description": "Backend for CRM Munnay",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "node node_modules/typescript/bin/tsc",
    "start": "node dist/index.js",
    "deploy": "./deploy.sh",
    "prisma:seed": "node --max-old-space-size=8192 node_modules/ts-node/dist/bin.js prisma/seed.ts",
